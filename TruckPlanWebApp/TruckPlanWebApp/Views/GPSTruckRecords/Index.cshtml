@model IEnumerable<TruckPlanApiClient.Model.GPSTruckRecordSelectDto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

 
<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.CurrentUser.FullName)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.GpsDeviceID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RecordDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Longitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Latitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CountryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kilometer)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.ProcessStatus)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.CurrentUser.FullName)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.GpsDeviceID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecordDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Longitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kilometer)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.ProcessStatus)
        </td>*@
        <td>
             @Html.ActionLink("Details", "Details", new { id=item.Id })  
           
        </td>
    </tr>
}

</table>
